#include "../github/includes/libft.h"
#include "../github/includes/types.h"
#include <stdio.h>

void print_stack_elements(t_stack *stack);
t_stack	*stack_new(int content);
void	stack_add_front(t_stack **stack, t_stack *new);
t_stack	*stack_last_node(t_stack *stack);
void	stack_add_back(t_stack **stack, t_stack *new);
void	clear_stack(t_stack **stack);

int main(int argc, char **argv)
{
	t_stack **stack_a;

	if (argc != 6)
		exit(1);
	stack_a = malloc(sizeof(t_stack **));
	*stack_a = stack_new(ft_atoi(argv[1]));
	print_stack_elements(*stack_a);
	stack_add_back(stack_a, stack_new(ft_atoi(argv[2])));
	print_stack_elements(*stack_a);
	stack_add_back(stack_a, stack_new(ft_atoi(argv[3])));
	print_stack_elements(*stack_a);
	stack_add_back(stack_a, stack_new(ft_atoi(argv[4])));
	print_stack_elements(*stack_a);
	stack_add_front(stack_a, stack_new(ft_atoi(argv[5])));
	print_stack_elements(*stack_a);
	clear_stack(stack_a);
}
